import java.util.Scanner;

public class ProgramaMedia {

public static void main(String[] args) {

Scanner teclado = new Scanner(System.in);
double nota1, nota2;
nota1 = teclado.nextDouble("insira a nota 1");
nota2 = teclado.nextDouble("Insira a nota 2");
media = ( nota1 + nota2 ) / 2;
System.out.println("A média entre as notas 1 e 2 é de: " + media);

}

}
